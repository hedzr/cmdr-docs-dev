{
  "name": "cmdr-docs",
  "version": "1.3.0",
  "private": true,
  "description": "my oss project docs site at docs.hedzr.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedzr/cmdr-docs-dev.git"
  },
  "author": "hedzr <hedzr@duck.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.17.1 || ^20.10.0 || >=22.11.0"
  },
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build0": "next build",
    "build0:posts": "node ./scripts/generate-posts-outline.mjs",
    "dev0": "next dev --turbo",
    "dev1": "next dev --turbo # https://nextjscn.org/docs/architecture/turbopack",
    "build1": "pnpm build:pre && next build && pnpm build:post",
    "clean": "rm -rf .source .next .vercel/output",
    "build:pre": "tsx ./scripts/pre-build.mts",
    "build:post": "tsx ./scripts/post-build.mts",
    "start:3001": "next start --port 3001",
    "build:with-mem-dbg": "next build --experimental-debug-memory-usage",
    "build:docs": "node ./scripts/generate-docs.mjs",
    "serve:dist": "python3 -m http.server --directory out -b 0.0.0.0 80 # after pnpm build",
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "eslint"
  },
  "dependencies": {
    "@orama/tokenizers": "^3.1.18",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-popover": "^1.1.15",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@vercel/toolbar": "^0.2.2",
    "class-variance-authority": "^0.7.1",
    "flags": "^4.0.3",
    "fumadocs-core": "16.6.2",
    "fumadocs-mdx": "14.2.7",
    "fumadocs-twoslash": "^3.1.13",
    "fumadocs-ui": "16.6.2",
    "katex": "^0.16.28",
    "lucide-react": "^0.564.0",
    "mermaid": "^11.12.2",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.9.17",
    "octokit": "^5.0.5",
    "posthog-js": "^1.347.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-medium-image-zoom": "^5.4.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.4.1",
    "twoslash": "^0.3.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@next/third-parties": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "next-validate-link": "^1.6.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
