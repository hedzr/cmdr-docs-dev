{
  "name": "cmdr-docs",
  "version": "2.0.0",
  "private": true,
  "description": "cmdr docs v2, doc site for hedzr.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hedzr/cmdr-docs-dev.git"
  },
  "author": "hedzr <hedzr@duck.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.17.1 || ^20.10.0 || >=22.11.0"
  },
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "dev": "next dev --turbopack # https://nextjscn.org/docs/architecture/turbopack",
    "build": "pnpm build:pre && next build && pnpm build:post",
    "clean": "rm -rf .source .next .vercel/output",
    "build:pre": "tsx ./scripts/pre-build.mts",
    "build:post": "tsx ./scripts/post-build.mts",
    "build0": "next build",
    "build0:posts": "node ./scripts/generate-posts-outline.mjs",
    "start": "next start",
    "start:3001": "next start --port 3001",
    "build:with-mem-dbg": "next build --experimental-debug-memory-usage",
    "postinstall": "fumadocs-mdx",
    "serve-dist": "python3 -m http.server --directory out -b 0.0.0.0 80 # after pnpm build",
    "build:docs": "node ./scripts/generate-docs.mjs"
  },
  "dependencies": {
    "@fumadocs/mdx-remote": "^1.2.1",
    "@orama/orama": "^3.1.3",
    "@orama/stopwords": "^3.1.3",
    "@orama/tokenizers": "^3.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@theguild/remark-mermaid": "^0.2.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "15.0.15",
    "fumadocs-mdx": "11.5.6",
    "fumadocs-openapi": "^6.3.0",
    "fumadocs-ui": "15.0.15",
    "geist": "^1.3.1",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "katex": "^0.16.21",
    "lucide-react": "^0.477.0",
    "next": "15.2.3",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "posthog-js": "^1.232.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-medium-image-zoom": "^5.2.14",
    "react-paginate": "^8.3.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-code-titles": "^1.2.0",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.0",
    "remark-rehype": "^11.1.1",
    "remark-stringify": "^11.0.0",
    "shiki": "^3.2.1",
    "tailwind-merge": "^3.0.2",
    "ts-morph": "^25.0.1",
    "unist-util-visit": "^5.0.0",
    "use-debounce": "^10.0.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@fumadocs/cli": "^0.0.8",
    "@next/bundle-analyzer": "^15.2.4",
    "@next/env": "^15.2.4",
    "@next/eslint-plugin-next": "^15.2.4",
    "@next/third-parties": "^15.2.4",
    "@tailwindcss/postcss": "^4.0.15",
    "@tailwindcss/typography": "0.5.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.13.8",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "next-validate-link": "^1.5.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.15",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "core-js",
      "esbuild",
      "rehype-code-titles",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@vercel/speed-insights"
    ]
  }
}
